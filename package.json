{
  "name": "site-template",
  "description": "Web App Template",
  "version": "0.0.1",
  "scripts": {
    "start:dev": "sapper dev",
    "prep:dev": "[ -d \"test\" ] && mv -vn test src/routes/test || true",
    "dev": "run-s prep:dev start:dev",
    "prettier": "prettier --write src/client.js src/server.js 'src/{routes,components}/**/*.{js,css,svelte}' rollup.config.js",
    "start:build": "sapper build --legacy",
    "prep:build": "[ -d \"src/routes/test\" ] && mv -vn src/routes/test test || true",
    "build:dev": "NODE_ENV=staging run-s prep:build start:build",
    "build": "NODE_ENV=production run-s prep:build start:build",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "passport": "^0.4.1",
    "passport-auth0": "^1.2.1",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/runtime": "^7.8.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.7.3",
    "cookie-parser": "^1.4.4",
    "cssnano": "^4.1.10",
    "cypress": "^3.8.2",
    "cypress-plugin-snapshots": "^1.2.9",
    "date-fns": "^2.9.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "fuse.js": "^3.4.6",
    "immer": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^1.6.1",
    "nanoid": "^2.1.9",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "purgecss-from-svelte": "^2.0.2",
    "rollup": "^1.29.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-root-import": "^0.2.4",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "sapper": "^0.27.9",
    "svelte": "^3.16.7",
    "svelte-preprocess": "^2.16.0",
    "tailwindcss": "^1.1.4"
  }
}
